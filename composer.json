{
    "name": "djkoza\/jarves",
    "description": "An open-source Content-Management-System CMS as Symfony Bundle.",
    "keywords": [
        "CMS",
        "PHP CMS",
        "WCMS"
    ],
    "license": "LGPL-3.0",
    "homepage": "http:\/\/jarves.io\/",
    "authors": [
        {
            "name": "Marc J. Schmidt",
            "homepage": "http://marcjschmidt.de",
            "email": "marc@marcjschmidt.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jarves\\": ""
        }
    },
    "require": {
        "php": ">=5.6",
        "symfony/symfony": "^2.8|^3.0",
        "twig/extensions": ">=1.0",
        "symfony/monolog-bundle": ">=2.8",
        "sensio/distribution-bundle": ">=2.3",
        "sensio/framework-extra-bundle": ">=2.3",
        "incenteev/composer-parameter-handler": ">=2.0",

        "propel/propel-bundle": "3.0.0-alpha1",
        "propel/propel": "2.0.0-alpha6",
        "sybio/image-workshop": ">=2",
        "michelf/php-markdown": "^1.6",
        "composer\/composer": "1.0.0",
        "friendsofsymfony/rest-bundle": "2.1.0",
        "nelmio/api-doc-bundle": "~2.5",
        "icap/html-diff": ">=1.0.1",
        "kadet/keylighter": "^0.8.2",
        "league/flysystem": "^1.0"
    },
    "minimum-stability": "alpha",
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "mockery/mockery": "@stable",
        "jarves/demotheme-bundle": "dev-master",
        "jarves/publication-bundle": "dev-master"
    },
    "suggest": {
        "kadet/keylighter": "Enabling code syntax highlighting in markdown"
    }
}
